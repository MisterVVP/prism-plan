{
    "version": "2.0",
    "concurrency": { 
        "dynamicConcurrencyEnabled": true, 
        "snapshotPersistenceEnabled": true 
    },
    "extensions": {
        "queues": {
            "maxPollingInterval": "00:00:00.250",
            "visibilityTimeout" : "00:00:01",
            "batchSize": 16,
            "maxDequeueCount": 5,
            "newBatchThreshold": 8,
            "messageEncoding": "none"
        }
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.0.0, 5.0.0)"
    }
}
